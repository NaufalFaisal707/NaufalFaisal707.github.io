    <style>@import url('https://fonts.googleapis.com/css2?family=Fredoka:wght@300;400&display=swap');</style> 
</head>
<body style="background-color: #e2e2e2;">
    <div class="container">
        <!-- WELCOME CARD -->
        <div class="col-12 my-2">
            <div class="rounded-3 shadow-sm position-relative overflow-hidden" style="background-color: #fff;">
                <div class="images" style="filter: blur(4px);background-image: url('./source/media/bg/bg-6.jpg');background-size: cover;background-repeat: no-repeat;background-position: center;background-color: #fff;">
                </div>
                <div id="text-card" class="text-card text-center position-absolute start-50 translate-middle">
                    <h4 class="text-white fw-bold">W E L C O M E !</h4>
                    <h6 class="text-white">Official Naufal Faisal Portofolio site <i class="bi bi-patch-check-fill"></i></h6>
                </div>
                <div class="position-absolute shadow-sm" style="border-top-left-radius: .3rem;bottom: 0;right: 0px;padding: 5px 15px;background-color: #fff;">
                    <a id="tab-menu" class="cursor-pointer"><span id="text-menu" class="">Buka menu</span></a>
                </div>
                <div id="respond-menu" class="g-animation" style="height: 0;background-color: blanchedalmond;">
                    cok
                </div>
            </div>
        </div>
        <!-- Content Loader -->
        <div id="root">
            <!-- Jika browser tidak mendukung JavaScript -->
            <noscript class="d-flex justify-content-center align-items-center text-center" style="height: 300px;">Sepertinya browser yang di gunakan tidak mendukung JavaScript...</noscript>
        </div>
    </div>
    <script>
        $('#root').load(progress);
        $(document).ready(() => {
            document.title = 'Naufal Faisal Portofolio';
            $('#root').load('/source/script/defaults.html');
            $('[data-age]').html(new Date().getFullYear() - 2005);
            $('#tab-menu').click(() => {
                $('#text-menu').html() == 'Buka menu' ? $('#text-menu').html('Tutup menu') : $('#text-menu').html('Buka menu');
                $('#respond-menu').toggleClass('pb-max');
            });
        });
    </script>
</body>
</html>